{
  "name": "@gameap/debug",
  "version": "0.2.17",
  "type": "module",
  "description": "Debug harness for GameAP plugin development with mock API",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "msw:init": "npx msw init ../../public --save",
    "sync:translations": "cp ../../../internal/i18n/en.json src/mocks/translations-en.json && cp ../../../internal/i18n/ru.json src/mocks/translations-ru.json"
  },
  "bin": {
    "gameap-debug": "./bin/cli.js"
  },
  "files": [
    "bin",
    "src",
    "public",
    "vendor",
    "index.html",
    "vite.config.ts",
    "tsconfig.json",
    "postcss.config.cjs",
    "empty-plugin"
  ],
  "dependencies": {
    "@gameap/plugin-sdk": "^0.2.0",
    "@gameap/frontend": "4.1.0-dev8",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@tailwindcss/postcss": "^4.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "msw": "^2.7.0",
    "postcss-preset-env": "^9.5.0",
    "sass": "^1.32.0",
    "tailwindcss": "^4.1.0",
    "vite": "^7.1.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "vue-router": "^4.6.0",
    "pinia": "^3.0.0",
    "naive-ui": "^2.43.0",
    "axios": "^1.12.0"
  },
  "msw": {
    "workerDirectory": [
      "../../public",
      "public"
    ]
  }
}
